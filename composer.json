{
  "name": "yurigiovani/code-database",
  "description": "Repository pattern Library based in Laravel 5",
  "license": "MIT",
  "keywords": [
    "laravel",
    "repository",
    "repository pattern"
  ],
  "authors": [{
      "name": "Yuri Giovani",
      "email": "yurigiovani.dias@gmail.com"
  }],
  "require": {
    "php": ">=5.5.9",
    "illuminate/database": "~5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "orchestra/testbench": "~3.0",
    "mockery/mockery": "~0.9.2"
  },
  "autoload": {
    "psr-4": {
      "CodePress\\CodeDatabase\\": "src/CodeDatabase/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "src/resources/migrations"
    ]
  }
}